{
  "kind": "collectionType",
  "collectionName": "communities",
  "info": {
    "singularName": "community",
    "pluralName": "communities",
    "displayName": "Community",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "Name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "default": "linkme-"
    },
    "Description": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text"
    },
    "Platform": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "online",
        "offline"
      ],
      "default": "online",
      "required": true
    },
    "CommunityID": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid",
      "required": true,
      "targetField": "Name"
    },
    "testUID": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid"
    },
    "MemberCount": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "biginteger"
    },
    "IsActive": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "boolean",
      "default": true,
      "required": true
    },
    "submittedBy": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "submittedCommunities"
    },
    "members": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "joinedCommunities"
    }
  }
}
